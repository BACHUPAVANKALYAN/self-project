#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/nodejs/sessions/Introduction-to-Node-JS/my-sequelize-project/node_modules/.pnpm/mkdirp@2.1.6/node_modules/mkdirp/dist/cjs/src/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/my-sequelize-project/node_modules/.pnpm/mkdirp@2.1.6/node_modules/mkdirp/dist/cjs/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/my-sequelize-project/node_modules/.pnpm/mkdirp@2.1.6/node_modules/mkdirp/dist/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/my-sequelize-project/node_modules/.pnpm/mkdirp@2.1.6/node_modules/mkdirp/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/my-sequelize-project/node_modules/.pnpm/mkdirp@2.1.6/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/my-sequelize-project/node_modules/.pnpm/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/my-sequelize-project/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/node_modules:/home/workspace/nodejs/sessions/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/my-sequelize-project/node_modules/.pnpm/mkdirp@2.1.6/node_modules/mkdirp/dist/cjs/src/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/my-sequelize-project/node_modules/.pnpm/mkdirp@2.1.6/node_modules/mkdirp/dist/cjs/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/my-sequelize-project/node_modules/.pnpm/mkdirp@2.1.6/node_modules/mkdirp/dist/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/my-sequelize-project/node_modules/.pnpm/mkdirp@2.1.6/node_modules/mkdirp/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/my-sequelize-project/node_modules/.pnpm/mkdirp@2.1.6/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/my-sequelize-project/node_modules/.pnpm/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/my-sequelize-project/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/node_modules:/home/workspace/nodejs/sessions/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cjs/src/bin.js" "$@"
else
  exec node  "$basedir/../../dist/cjs/src/bin.js" "$@"
fi

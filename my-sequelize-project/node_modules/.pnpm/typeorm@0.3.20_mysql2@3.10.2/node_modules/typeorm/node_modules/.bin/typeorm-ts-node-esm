#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/nodejs/sessions/Introduction-to-Node-JS/my-sequelize-project/node_modules/.pnpm/typeorm@0.3.20_mysql2@3.10.2/node_modules/typeorm/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/my-sequelize-project/node_modules/.pnpm/typeorm@0.3.20_mysql2@3.10.2/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/my-sequelize-project/node_modules/.pnpm/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/my-sequelize-project/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/node_modules:/home/workspace/nodejs/sessions/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/my-sequelize-project/node_modules/.pnpm/typeorm@0.3.20_mysql2@3.10.2/node_modules/typeorm/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/my-sequelize-project/node_modules/.pnpm/typeorm@0.3.20_mysql2@3.10.2/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/my-sequelize-project/node_modules/.pnpm/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/my-sequelize-project/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/node_modules:/home/workspace/nodejs/sessions/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../../cli-ts-node-esm.js" "$@"
fi
